{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": "./index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "DEBUG=@neo4j/graphql:* dotenv -e .env -- tsup --watch --onSuccess \"node dist/index.js\"",
    "build": "dotenv -e .env -- tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "introspect": "dotenv -e .env -- tsup src/introspect.ts --onSuccess \"node dist/introspect.js\""
  },
  "dependencies": {
    "@neo4j/graphql": "3.24.3",
    "@neo4j/introspector": "1.0.3",
    "apollo-server": "3.12.1",
    "graphql": "16.8.1",
    "neo4j-driver": "4.4.11"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "dotenv-cli": "7.3.0",
    "eslint-config-custom": "*",
    "tsconfig": "*",
    "tsup": "7.2.0",
    "typescript": "^4.5.3"
  }
}
